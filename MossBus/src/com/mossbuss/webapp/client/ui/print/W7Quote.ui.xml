<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.skyrat.w8.client.ui.customers" xmlns:p2="urn:import:com.skyrat.w8.client.ui.general.Panels" xmlns:p3="urn:import:com.skyrat.w8.client.ui.stock" xmlns:p4="urn:import:com.skyrat.w8.client.ui.sales.quotes">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel width="1024px" height="768px" ui:field="pagepanel">
		<g:TabPanel width="903px" height="607px" ui:field="tabPanel">
			<g:Tab text="Search">
				<g:AbsolutePanel width="1024px" height="768px" ui:field="absWS">
					<g:at left="10" top="10">
						<p4:QuoteSearchPanel ui:field="quoteSearch"/>
					</g:at>
				</g:AbsolutePanel>
			</g:Tab>
			<g:Tab text="Customer">
				<g:AbsolutePanel width="1024px" height="768px">
					<g:at left="10" top="0">
						<p1:CustomerDetailsPanel ui:field="customerDetailsPanel" width="884px" height="563px"/>
					</g:at>
				</g:AbsolutePanel>
			</g:Tab>
			<g:Tab text="Quote Settings">
				<g:AbsolutePanel width="942px" height="554px">
					<g:at left="10" top="10">
						<g:Label text="Quote Heading:"/>
					</g:at>
					<g:at left="120" top="7">
						<g:TextBox width="819px" height="15px" styleName="generalText" ui:field="quoteHeadingText"/>
					</g:at>
					<g:at left="10" top="44">
						<g:Label text="Delivery Time:"/>
					</g:at>
					<g:at left="120" top="41">
						<g:TextBox width="819px" height="15px" styleName="generalText" ui:field="deliveryTimeText"/>
					</g:at>
					<g:at left="10" top="79">
						<g:Label text="Exchange Rate:"/>
					</g:at>
					<g:at left="120" top="76">
						<g:TextBox width="275px" height="15px" styleName="generalText" ui:field="rateOfExchangeText"/>
					</g:at>
					<g:at left="778" top="68">
						<p2:UserSelectPanel ui:field="spSelection" width="143px" height="67px"/>
					</g:at>
					<g:at left="10" top="113">
						<g:Label text="Payment Terms:"/>
					</g:at>
					<g:at left="10" top="137">
						<p2:RichTextEditPanel width="421px" height="243px" ui:field="paymentTermsText" hTML="COD unless account is held. All ownership of equipment or goods remains property of Wirsam Scientific until paid in full. Prepayment on ordering for all export orders."/>
					</g:at>
					<g:at left="120" top="103">
						<g:Button styleName="generalButton" ui:field="paymentTermsButton">Reset</g:Button>
					</g:at>
					<g:at left="449" top="113">
						<g:Label text="Installation terms:"/>
					</g:at>
					<g:at left="449" top="142">
						<p2:RichTextEditPanel width="421px" height="243px" ui:field="installationTermsText" hTML="Prices quoted exclude installation and training."/>
					</g:at>
					
					<g:at left="558" top="103">
						<g:Button width="66px" height="28px" styleName="generalButton" text="Excluded" ui:field="exclButton">Reset</g:Button>
					</g:at>
					<g:at left="630" top="103">
						<g:Button width="66px" height="28px" styleName="generalButton" text="Included" ui:field="inclButton">Reset</g:Button>
					</g:at>
					<g:at left="683" top="79">
						<g:Label text="Sales Person:" styleName="boldLabel"/>
					</g:at>
					<g:at left="405" top="69">
						<g:Button styleName="generalButton" ui:field="exchangeButton">N/A</g:Button>
					</g:at>
					
				</g:AbsolutePanel>
			</g:Tab>
			<g:Tab text="Quote Details">
				<g:AbsolutePanel width="1024px" height="768px" ui:field="quoteDetailsTab">
					<g:at left="10" top="10">
						<p3:StockSearchPanel width="800px" height="81px" ui:field="stockSearch"/>
					</g:at>
					<g:at left="10" top="102">
						<g:ScrollPanel width="800px" height="361px" ui:field="scrollPanel"/>
					</g:at>
					<g:at left="734" top="50">
						<g:Button styleName="generalButton" ui:field="buttonFinalize">Finalize</g:Button>
					</g:at>
					<g:at left="649" top="50">
						<g:Label text="New Quote" ui:field="wsNumberLabel" styleName="appHeading"/>
					</g:at>
					<g:at left="517" top="50">
						<g:CheckBox text="VAT Exempt" ui:field="vatExempt"/>
					</g:at>
					<g:at left="517" top="22">
						<g:ListBox visibleItemCount="1" ui:field="Currency" styleName="generalText"/>
					</g:at>
					<g:at left="92" top="63">
						<g:Button text="Comment" styleName="generalButton" ui:field="buttonComment"/>
					</g:at>
					<g:at left="174" top="63">
						<g:Button text="Discount" styleName="generalButton" ui:field="buttonDiscount"/>
					</g:at>
					<g:at left="773" top="18">
						<g:Button styleName="printButton" ui:field="printQuote"/>
					</g:at>
				</g:AbsolutePanel>
			</g:Tab>
			<g:Tab text="Options">
				<g:AbsolutePanel width="1024px" height="768px" ui:field="quoteOptionsTab">
					<g:at left="10" top="10">
						<p3:StockSearchPanel width="504px" height="78px" ui:field="optionStockSearch"/>
					</g:at>
					<g:at left="10" top="99">
						<g:ScrollPanel width="800px" height="361px" ui:field="optionsScrollPanel"/>
					</g:at>
				</g:AbsolutePanel>
			</g:Tab>
			<g:Tab text="Sales Order">
				<g:FlowPanel width="527px" height="357px" ui:field="soDataPanel">
					<p2:SalesOrderEntryPanel ui:field="soEditPanel"/>
					<p2:SalesOrderViewPanel ui:field="soView"/>
				</g:FlowPanel>
			</g:Tab>
		</g:TabPanel>
	</g:HTMLPanel>
</ui:UiBinder> 