<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel height="30px">
		<g:HorizontalPanel width="352px">
			<g:HorizontalPanel width="252px" height="30px">
				<g:Label text="New Label" ui:field="usernameLabel"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel height="30px" width="84px">
				<g:Button ui:field="editButton" text="Edit"/>
				<g:Button ui:field="deleteButton" text="Del"/>
			</g:HorizontalPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 