<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:TabPanel width="500px" height="299px" styleName="gwt-DialogBox"
			ui:field="tPanel">
			<g:Tab text="Bus Details">
				<g:AbsolutePanel width="451px" height="257px">
					<g:at left="10" top="10">
						<g:Label text="Bus Edit" width="371px" height="18px"
							horizontalAlignment="ALIGN_CENTER" styleName="headingText" />
					</g:at>

					<g:at left="10" top="55">
						<g:Label text="Bus name:" />
					</g:at>
					<g:at left="72" top="55">
						<g:TextBox width="348px" height="6px" ui:field="nameField" />
					</g:at>
				






					<g:at left="10" top="206">
						<g:Button styleName="generalButton" ui:field="cancelButton">Cancel</g:Button>
					</g:at>

					<g:at left="196" top="179">
						<g:Label width="234px" height="18px" ui:field="errorLabel"
							styleName="serverResponseLabelError" />
					</g:at>
					<g:at left="350" top="206">
						<g:Button  ui:field="saveButton"
							styleName="generalButton" text="Save">Next</g:Button>
					</g:at>
				</g:AbsolutePanel>
			</g:Tab>
		</g:TabPanel>
	</g:HTMLPanel>
</ui:UiBinder> 